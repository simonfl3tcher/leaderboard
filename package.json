{
  "name": "leaderboard",
  "description": "A React playground project",
  "author": "Simon Fletcher <simonfletcher0@gmail.com>",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "server": "cross-env NODE_ENV=development webpack-dev-server --port 2000 --open --inline --hot",
    "server:quite": "webpack-dev-server --port 2000 --quiet --no-info",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "mocha \"src/**/*.spec.js\"",
    "nightwatch": "./bin/boot-nightwatch.sh",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  },
